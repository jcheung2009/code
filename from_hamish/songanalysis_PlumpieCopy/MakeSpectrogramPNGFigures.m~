function [] = MakeSpectrogramPNGFigures(DirectoryName, NoteFileDirectoryName, FileList, FileType, NumberToPlot)

PresentDir = pwd;

FileSep = filesep;

if (DirectoryName(end) ~= FileSep)
    DirectoryName(end+1) = FileSep;
end

cd(DirectoryName);
!mkdir PNGSpectrograms

Fid = fopen(FileList, 'r');
Temp = textscan(Fid, '%s', 'DeLimiter', '\n');
Temp = Temp{1};

for i = 1:min(NumberToPlot, length(Temp)),
    PlotSpectrogram(DirectoryName, Temp{i}, FileType, 'hot');
    title('');
    plot([0 1], [301 301], 'k', 'LineWidth', 2);
    set(gca, 'Visible', 'off');
    load([NoteFileDirectoryName, FileSep, Temp{i}, '.not.mat']);
    hold on;
    for j = 1:length(labels),
        text((onsets(j) + offsets(j))/2000, 7900, labels(j), 'FontSize', 18, 'FontWeight', 'bold', 'Color', 'b');
    end
    OutputFileName = [Temp{i}, '.png'];
    cd('PNGSpectrograms');
    saveas(gcf, '
end

disp('Finished');
