function [filtered,minvect,maxvect] = counterfilt(rawtrigs,mincount,maxcount)
%
% mnmn, 19 april 2009
%

filtered = zeros(length(rawtrigs),1);
minvect = zeros(length(rawtrigs),1);
maxvext = zeros(length(rawtrigs),1);

for index=1:length(rawtrigs)-maxcount;
    min = index + mincount-1;
    minvect(index) = sum(index:min);
    
    max = index + maxcount-1;
    maxvect(index) = ;
    
    if (rawtrigs(index) && sum(rawtrigs(index:min)) == mincount && sum(rawtrigs(index:max)) <= maxcount);
        filtered(index) = 1;
    end

end


