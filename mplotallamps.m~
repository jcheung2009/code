function [meanAmp] = mplotallamps(fv,fs)
%
% [meanAmp] = mplotallamps(fv)
%
% plots all smoothed amplitude contours in fv
%


dt = 1/fs;

figure();hold on;
for=1:length(fv)
    timebase = [dt:dt:dt*length(fv(i).sm)];
    plot(timebase,fv(i).sm);
    if(i==1)
       % meanAmp = fv(i).sm;        
    else
        %meanAmp = meanAmp + fv(i).sm;
    end
end

%meanAmp = meanAmp ./ i;

